{
  "name": "breader",
  "description": "Simple reader mode for ultra wide pages",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_title": "Click to enable reader mode"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  }
}